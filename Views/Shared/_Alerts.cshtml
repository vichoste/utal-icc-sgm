		</div>
	</div>
}
@if (this.TempData["WarningMessages"] is not null) {
	@foreach (var warning in (IEnumerable<string>)this.TempData["WarningMessages"]!) {
		<div class="row justify-content-center">
			<div class="alert alert-warning" role="alert">
				@warning
			</div>
		</div>
	}
}
